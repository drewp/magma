{
    "src": "./",
    "dest": "build/",
    "patterns": "[a-z]*{.jade,.coffee,.styl} components/[a-z]*{.coffee,.styl,.jade}",
    "copy": "*.css *.svg *.png",
    "jade": {
        "pretty": true
    },
    "coffee": {
        "bare": true
    },
    "styl": {
        "u": "autoprefixer-stylus",
        "compress": false,
        "nib": true
    },
    "delete": true,
    static: false,
    livereload: false
}
