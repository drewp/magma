{ 
  ?user a :CurrentUser; :seesCommand ?cmd . 
} => { 
  ?cmd a :available .
} .

{ 
  supbang:cmd_backupPreDrew :state state:EXITED .
} => { 
  cmd:stopBackupPreDrew :ranking -1 .
} .

{:ari a :sleep .} =>  {cmd:slashPandoraToddler :ranking -1 .} .
{:outside a :light } => { cmd:frontLightOn :ranking -1 . } .
{:outside a :light } => { cmd:frontLightOff :ranking 10 . } .
#{not on? } => { cmd:frontLightOff :ranking -1 . } .
#{:outside a :dark } => { cmd:frontLightOff :ranking 0 . } .

{ 
  <http://projects.bigasterisk.com/device/heater> :state :off . 
} => { 
  <http://bigasterisk.com/magma/cmd/heaterOff> :ranking -1 .
} .

